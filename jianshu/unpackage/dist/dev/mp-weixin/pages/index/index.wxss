page {
  width: 100%;
  background-color: #F4F4F4;
}
.trade {
  width: 100%;
  color: #a6a6a6;
  overflow: auto;
  background-color: white;
}
.trade view {
  text-align: center;
  padding: 10rpx 0 22rpx 0;
  width: 50%;
  float: left;
}
.trade .texts.active {
  border-bottom: 8rpx solid #0f6cd6;
  color: #010101;
}
.model-back {
  height: 100%;
  border-radius: 8rpx;
  margin: 22rpx 0 6rpx 0;
  padding: 18rpx 15rpx 14rpx 23rpx;
  background-color: white;
  box-shadow: 0 3rpx 6rpx rgba(0, 0, 0, 0.2);
}
.model-back .model-back-title {
  font-size: 14px;
  font-weight: bold;
  letter-spacing: 2px;
}
.model-back .model-back-user-info {
  position: relative;
}
.model-back .model-back-name-introduce name {
  width: 100%;
}
.model-back .model-back-name-introduce {
  position: absolute;
  top: 15rpx;
  left: 59rpx;
  font-size: 13px;
}
.model-back .model-back-name-introduce ._span {
  margin-left: 10rpx;
  color: #c9c9c9;
}
.model-back .model-back-user-info image {
  width: 45rpx;
  height: 45rpx;
  border-radius: 50%;
  margin: 10rpx 10rpx 10rpx 0;
}
.model-back .model-back-content {
  font-size: 14px;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.model-back .model-back-agree-comment {
  padding: 10rpx 0 10rpx 0;
}
.model-back .model-back-agree-comment .agree,
.model-back .model-back-agree-comment .comment {
  font-size: 14px;
  color: #c9c9c9;
}
.model-back .model-back-agree-comment .dian {
  font-size: 12px;
  padding: 0 10rpx 0 10rpx;
  color: #c9c9c9;
}

